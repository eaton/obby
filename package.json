{
  "name": "obby",
  "version": "1.2.0",
  "description": "Manipulate, clone, merge, and compare Javascript objects with ease.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "/dist",
    "README.md"
  ],
  "scripts": {
    "build": "pkgroll --minify",
    "clean": "shx rm -rf dist;shx rm -rf .tsimp",
    "prepare": "npm run clean && npm run build",
    "test": "ava"
  },
  "ava": {
    "files": [
      "./test/**/*",
      "!./test/fixtures"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--import=tsimp/import"
    ]
  },
  "keywords": [
    "object",
    "dot-path",
    "dot-notation",
    "typescript",
    "clone",
    "deep-clone",
    "merge",
    "deep-merge",
    "clone",
    "deep-clone",
    "compare",
    "deep-compare",
    "deep-equals"
  ],
  "author": "eaton",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/eaton/obby.git"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "ava": "^6.1.2",
    "pkgroll": "^2.0.2",
    "shx": "^0.3.4",
    "tsimp": "^2.0.11",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "deepmerge-ts": "^5.1.0",
    "fast-copy": "^3.0.2",
    "fast-equals": "^5.0.1",
    "ts-dot-prop": "^2.1.3"
  }
}
